{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
    },
    "removeComments": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
        "components/*": [
            "src/components/*"
        ],
    }
}